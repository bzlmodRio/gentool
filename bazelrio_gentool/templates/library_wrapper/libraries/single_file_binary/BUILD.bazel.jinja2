alias(
    name = "{{target.tool_name}}",
    actual = select({
        "@rules_bzlmodrio_toolchains//conditions:linux_x86_64": "@{{target.get_archive_name("linux-x86_64")}}//file:downloaded",
        "@rules_bzlmodrio_toolchains//conditions:osx": "@{{target.get_archive_name("macos-x86_64")}}//file:downloaded",
        "@rules_bzlmodrio_toolchains//conditions:windows": "@{{target.get_archive_name("windows-x86_64.exe")}}//file:downloaded",
    }),
)

