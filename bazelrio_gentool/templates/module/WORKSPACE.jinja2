workspace(name = "{{group.sanitized_repo_name}}")

load("@{{group.sanitized_repo_name}}//private/non_bzlmod_dependencies:download_dependencies.bzl", "download_dependencies")

download_dependencies()

load("@{{group.sanitized_repo_name}}//private/non_bzlmod_dependencies:setup_dependencies.bzl", {% if group.has_direct_maven_deps() %}"get_java_dependenicies", {% endif %}"setup_dependencies")

setup_dependencies()

load("@rules_jvm_external//:defs.bzl", "maven_install")

maven_artifacts, maven_repositories = {% if group.has_any_maven_deps() %}get_java_dependenicies(){% else %}[], [
    "https://repo1.maven.org/maven2",
    "https://frcmaven.wpi.edu/release",
]{%endif%}

maven_install(
    name = "maven",
    artifacts = maven_artifacts,
    repositories = maven_repositories,
    # maven_install_json = "//build_scripts/bazel/deps:maven_install.json",
)

load("@bzlmodrio-gentool-pip//:requirements.bzl", "install_deps")

install_deps()

